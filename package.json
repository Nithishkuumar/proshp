{
  "homepage": "https://Nithishkuumar.github.io/shopelectronic",
  "name": "commerce",
  "version": "1.0.0",
  "description": "E-commerce ",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node Backend/Server.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "nodemon Backend/Server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node Backend/Seeder.js",
    "data:destroy": "node Backend/Seeder.js -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Nithish",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "stripe": "^14.16.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.0.2"
  }
}
